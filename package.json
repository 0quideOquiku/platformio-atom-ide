{
  "name": "platformio-ide",
  "main": "./lib/main",
  "version": "2.0.0-alpha.4",
  "description": "Official PlatformIO IDE for Atom. PlatformIO is an open source ecosystem for IoT development. Cross-platform build system and library manager. Continuous and IDE integration. Arduino and ARM mbed compatible.",
  "keywords": [
    "PlatformIO",
    "IoT",
    "embedded",
    "arduino",
    "mbed",
    "flash",
    "firmware",
    "esp8266",
    "esp32",
    "avr",
    "arm",
    "fpga",
    "verilog",
    "hardware"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platformio-atom-ide.git"
  },
  "bugs": {
    "url": "https://github.com/platformio/platformio-atom-ide/issues"
  },
  "license": "GPL-2.0",
  "engines": {
    "atom": ">=1.12.2 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "builder": {
      "description": "PlatformIO IDE for Atom",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "<2.0.0": "consumeToolBar"
      }
    },
    "runInTerminal": {
      "versions": {
        "^0.14.5": "consumeRunInTerminal"
      }
    },
    "busy": {
      "versions": {
        "^1.0.0": "consumeBusy"
      }
    }
  },
  "dependencies": {
    "atom-select-list": "0.0.11",
    "command-join": "1.0.1",
    "dexie": "1.5.1",
    "etch": "0.8.0",
    "fs-extra": "0.26.5",
    "fuzzaldrin": "2.1.0",
    "ini": "1.3.4",
    "lockfile": "1.0.1",
    "promisify-node": "0.4.0",
    "relative-date": "1.1.3",
    "request": "2.79.0",
    "semver": "5.3.0",
    "tar": "2.2.1",
    "temp": "0.8.3"
  },
  "devDependencies": {
    "babel-eslint": "~7.1.1",
    "esformatter": "~0.10.0",
    "esformatter-jsx": "~7.4.0",
    "eslint": "~3.12.2",
    "eslint-plugin-jsx": "~0.0.2"
  }
}
