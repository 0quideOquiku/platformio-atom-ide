{
  "name": "platformio-ide",
  "main": "./lib/main",
  "version": "0.3.1",
  "description": "Official PlatformIO IDE for Atom. PlatformIO is an open source ecosystem for IoT development. Cross-platform code builder and library manager. Continuous and IDE integration. Arduino and MBED compatible. Ready for Cloud compiling.",
  "keywords": [
    "PlatformIO",
    "IoT",
    "embedded",
    "arduino",
    "mbed",
    "build",
    "library manager"
  ],
  "repository": "https://github.com/platformio/platformio-atom-ide",
  "license": "GPL-2.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "builder": {
      "description": "PlatformIO IDE for Atom",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0.1.0": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^3.0.7",
    "ini": "^1.3.4",
    "rimraf": "^2.5.0",
    "tarball-extract": "^0.0.3",
    "temp": "^0.8.3"
  },
  "package-deps": [
    "build",
    "autocomplete-clang",
    "linter",
    "linter-gcc",
    "platformio-ide-terminal",
    "language-ini",
    "tool-bar"
  ]
}
